publishedAspectsOfInstances
	"Answer a <LookupTable> of the <Aspect>s published by instances of the receiver."

	| dayMap days |
	dayMap := LookupTable new.
	days := (1 to: 7) collect: 
					[:each |
					| day |
					day := Locale default dayOfWeekName: (each + 1) \\ 7.
					dayMap at: day put: each.
					day].
	^super publishedAspectsOfInstances
		add: (Smalltalk.Aspect boolean: #hasToday);
		add: (Smalltalk.Aspect boolean: #hasTodayCircle);
		add: (Smalltalk.Aspect boolean: #hasWeekNumbers);
		add: (Smalltalk.Aspect name: #firstDayOfWeek
					presenterBlock: 
						[:parent :model |
						(ChoicePresenter createIn: parent
							on: (ValueConverter subject: model
									typeConverter: ((MappingConverter rightToLeft: dayMap)
											leftNullValue: 0;
											yourself)))
							choices: days;
							yourself]);
		yourself! !
