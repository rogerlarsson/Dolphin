	aspects := super publishedAspects.
	self bitmap
		ifNotNil: 
			[:bmp |
			aspects add: (Smalltalk.Aspect choice: #bitmapIndex
						from: (0 to: bmp extent x // (self toolbar ifNil: [16] ifNotNil: [:tb | tb actualBitmapSize x])))].
	^aspects! !
